{
  "name": "Zoom Bookmarks Demo",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Demo bookmarks app that allows you to save and open your Zoom links.",
  "homepage_url": "https://hackclub.com",
  "icons": {
    "48": "icon.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Zoom Bookmarks Demo",
    "default_popup": "index.html"
  },
  "permissions": [
    "chrome://favicon/",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "contextMenus",
    "cookies",
    "idle",
    "management",
    "notifications",
    "tabs",
    "background",
    "alarms",
    "storage"
  ]
}
